@model DotNetAppSqlDb.Models.Autorizacao
@{
    ViewBag.Title = "AutorizacaoAluno";
    Layout = "~/Views/Shared/AlunoView.cshtml";
}

<html>
<head>
    <link href="@Url.Content("~/Content/css/area-aluno.css")" rel="stylesheet" />
</head>
<body>
    <div class="form-group">
        <p class="estilo-texto2">Caso haja algum problema em sua autorização, será necessário enviar uma nova</p>
    </div>
    <div class="form-group">
        <label class="control-label col-md-10">URL da Sua Autorizacao</label>
        <div class="col-md-10">
            @Html.TextBoxFor(model => model.URL, new { @class = "form-control w-50", disabled = "disabled" })
        </div>
        <div class="col-md-10" style="margin-top:10px">
            @Html.ActionLink("Editar", "EditarAutorizacao", "Aluno", new { idAutorizacao = Model.IdAutorizacao }, new { @class = "btn btn-primary" })
        </div>
    </div>
</body>
</html>

